<form action="/logout" method="GET">
  <strong><label class="viewTitle">Bienvenido {{this.user}}</label></strong>
  <button class="myButton" type="submit">Desloguear</button>
</form>

<h1 class="viewTitle">Ingrese un Producto</h1>
<form method="post" action="/">
  <div><button class="myButton" type="submit" id="buttonProducts">Ingrese un
      nuevo producto fake</button></div>
</form>
<br />

<h1 class="viewTitle"> Vista de Productos </h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Precio</th>
    <th>Foto</th>
  </tr>
  {{#each data}}
    <tr>
      <td>{{this.title}}</td>
      <td>{{this.price}}</td>
      <td><img src="{{this.thumbnail}}" width="75" px height="auto" /></td>
    </tr>
  {{/each}}
</table>