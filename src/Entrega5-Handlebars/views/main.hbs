<h1 class="viewTitle"> Vista de Productos </h1>
 {{#if exists}}
<table>
    <tr>
        <th>Nombre</th>
        <th>Precio</th>
        <th>Foto</th>
    </tr>
    {{#each data}}
    <tr>
        <td>{{this.title}}</td>
        <td>{{this.price}}</td>
        <td><img src="{{this.thumbnail}}" width=75 px height=auto></td>
    </tr>
    {{/each}}
</table>
{{else}}
<h2 class="noProduct">No existen productos</h2>
{{/if}}


