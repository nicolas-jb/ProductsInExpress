<head>
  <meta charset="UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>ProductsInExpress</title>
  <link rel="stylesheet" type="text/css" href="../style.css" />

</head>

<body class="bgColor">

  <h1 class="viewTitle"> Lista de Productos Mock</h1>
  <table>
    <tr>
      <th>Nombre</th>
      <th>Precio</th>
      <th>Foto</th>
    </tr>
    {{#each productos}}
      <tr>
        <td> {{this.title}} </td>
        <td> {{this.precio}} </td>
        <td> <img src={{this.thumbnail}} width="75" px height="auto" /> </td>
      </tr>
    {{/each}}
  </table>

</body>